# Environment variables for mock-interview-hub
# Copy this file to .env and fill in your values

# Database URL (optional - defaults to SQLite for local development)
# DATABASE_URL=postgresql://user:password@host:5432/dbname

# Gemini API Key for AI features (get from https://ai.google.dev/)
GEMINI_API_KEY=your_gemini_api_key_here

# Secret key for JWT/sessions (generate a random string)
SECRET_KEY=your_secret_key_here 

# Frontend URL for CORS (optional - for production)
# FRONTEND_URL=https://your-frontend-url.onrender.com

# Disable seeding in production
# DISABLE_SEED=true
